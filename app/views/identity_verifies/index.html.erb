<h1>Listing identity_verifies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @identity_verifies.each do |identity_verify| %>
      <tr>
        <td><%= link_to 'Show', identity_verify %></td>
        <td><%= link_to 'Edit', edit_identity_verify_path(identity_verify) %></td>
        <td><%= link_to 'Destroy', identity_verify, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Identity verify', new_identity_verify_path %>

<div class="main-content-inner">
  <!-- #section:basics/content.breadcrumbs -->
  <div class="breadcrumbs" id="breadcrumbs">
    <script type="text/javascript">
      try{ace.settings.check('breadcrumbs' , 'fixed')}catch(e){}
    </script>

    <ul class="breadcrumb">
      <li>
        <i class="ace-icon fa fa-home home-icon"></i>
        <%= link_to "代理商", admin_agents_path %>
      </li>
      <li class="active">详细日志</li>
    </ul><!-- /.breadcrumb -->
  </div>

  <!-- /section:basics/content.breadcrumbs -->
  <div class="page-content">
    <div class="row panel panel-default">
      <table id="agents-table" class="table table-striped table-bordered table-hover dataTable" role="grid">
        <thead>
            <td>手机</td>
            <td>身份证</td>
            <td>姓名</td>
            <td>交易标示</td>
            <td>兑换积分</td>
            <td>错误原因</td>
          </thead>

        <tbody>
          <% @results.each do |result| %>
          <tr>
            <td><%= result["手机号"] %></td>
            <td><%= result["身份证号"] %></td>
            <td><%= result["姓名"] %></td>
            <td><%= result["交易的唯一标示"] %></td>
            <td><%= result["兑换积分数"] %></td>
            <td><%= result["错误原因"] %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
    <% content_for :scripts do %>
      <script type="text/javascript">
        $('#agents-table').dataTable({
          bAutoWidth: false
        });
      </script>
    <% end %>
  </div>  
</div>

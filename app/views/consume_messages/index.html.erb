<h1>Listing consume_messages</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consume_messages.each do |consume_message| %>
      <tr>
        <td><%= link_to 'Show', consume_message %></td>
        <td><%= link_to 'Edit', edit_consume_message_path(consume_message) %></td>
        <td><%= link_to 'Destroy', consume_message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consume message', new_consume_message_path %>
